TYPE a
    DECLARE i:REAL
ENDTYPE

FUNCTION factorial(a:REAL) RETURNS REAL
    IF a = 0.0
        THEN
            RETURN 1.0
    ENDIF
    RETURN a*factorial(a-1.0)
ENDFUNCTION

DECLARE b:a
b.i <- 5.0

OUTPUT factorial(b.i)

DECLARE a:INTEGER
a <- 10

WHILE a <> 0
    OUTPUT a
    a <- a-1
ENDWHILE